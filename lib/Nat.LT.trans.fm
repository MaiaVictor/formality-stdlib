Nat.LT.trans
: {a : (Nat a)} {b : (Nat b)} {c : (Nat c)} {p1 : (Nat.LT a b p1)} {p2 : (Nat.LT b c p2)} (Nat.LT a c (Nat.LT.trans a b c p1 p2))
= [a] [b] [c] [p1] [p2]
  let x1 (Nat.LT.fst a b p1)
  let x2 (Nat.LT.fst b c p2)
  let e1 (Nat.LT.snd a b p1)
  let e2 (Nat.LT.snd b c p2)
  let e3
    (Eq.trans Nat (Nat.add a (Nat.add x1 x2)) (Nat.add (Nat.add a x1) x2) c
      (Nat.add_assoc_r a x1 x2)
      (Eq.trans Nat
        (Nat.add (Nat.add a x1) x2) (Nat.add b x2) c (Eq.cong Nat Nat (Nat.add a x1) b e1 [x : (Nat x)] (Nat.add x x2)) e2))
  (Nat.LT.cons a c (Nat.add x1 x2) e3)
